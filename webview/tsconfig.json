{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ES2020",
        "lib": [
            "DOM",
            "ES2020"
        ],
        "jsx": "react",
        "sourceMap": true,
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "**/*"
    ]
}